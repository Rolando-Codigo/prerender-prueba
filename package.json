{
    "name": "heybanco",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --proxy-config proxy.config.json",
        "develop": "ng serve --disable-host-check --proxy-config proxy.config.json",
        "build": "npm run build:ssr",
        "launch": "npm run serve:ssr",
        "profiling": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && node --inspect dist/heybanco/server/main.js",
        "lint": "ng lint",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "dev:ssr": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && ng run heybanco:serve-ssr --proxy-config proxy-config.json",
        "serve:ssr": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && node dist/heybanco/server/main.js",
        "build:ssr": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && ng build --aot --build-optimizer --configuration production && ng run heybanco:server",
        "build:ssr:dev": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && ng build --configuration development && ng run heybanco:server",
        "prerender": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && node --max-old-space-size=1536 ./node_modules/@angular/cli/bin/ng run heybanco:prerender",
        "pm2-init": "pm2 startup",
        "pm2-save": "pm2 save",
        "pm2-restart": "pm2 restart all",
        "pm2-reload": "pm2 reload all",
        "pm2-delete": "pm2 delete all",
        "pm2-monit": "pm2 monit",
        "start-app": "pm2 start npm --name hey -- run serve:ssr"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~12.1.0",
        "@angular/cdk": "^12.1.4",
        "@angular/common": "~12.1.0",
        "@angular/compiler": "~12.1.0",
        "@angular/core": "~12.1.0",
        "@angular/flex-layout": "^12.0.0-beta.35",
        "@angular/forms": "~12.1.0",
        "@angular/http": "^7.2.16",
        "@angular/material": "^12.1.4",
        "@angular/platform-browser": "~12.1.0",
        "@angular/platform-browser-dynamic": "~12.1.0",
        "@angular/platform-server": "~12.1.0",
        "@angular/router": "~12.1.0",
        "@ng-bootstrap/ng-bootstrap": "^9.1.3",
        "@nguniversal/express-engine": "^12.1.0",
        "@nicky-lenaers/ngx-scroll-to": "^9.0.0",
        "@popperjs/core": "^2.4.4",
        "body-parser": "^1.19.0",
        "bootstrap": "^4.6.0",
        "compression": "^1.7.4",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-http-proxy": "^1.6.2",
        "frameguard": "^4.0.0",
        "fs": "0.0.1-security",
        "helmet": "^4.6.0",
        "jquery": "^3.5.1",
        "localstorage-polyfill": "^1.0.1",
        "ng-recaptcha": "^5.0.0-beta.2",
        "ngx-captcha": "^5.0.4",
        "pm2": "^5.2.0",
        "rxjs": "~6.6.0",
        "tslib": "^2.2.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~12.1.4",
        "@angular-eslint/builder": "12.7.0",
        "@angular-eslint/eslint-plugin": "12.7.0",
        "@angular-eslint/eslint-plugin-template": "^12.2.1",
        "@angular-eslint/schematics": "12.7.0",
        "@angular-eslint/template-parser": "^12.2.2",
        "@angular/cli": "~12.1.4",
        "@angular/compiler-cli": "~12.1.0",
        "@nguniversal/builders": "^12.1.0",
        "@types/express": "^4.17.0",
        "@types/jasmine": "~3.8.0",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "^4.28.5",
        "@typescript-eslint/parser": "^4.28.5",
        "eslint": "^7.31.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "jasmine-core": "~3.8.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "prettier": "^2.3.2",
        "prettier-eslint": "^13.0.0",
        "source-map-explorer": "^2.5.3",
        "typescript": "~4.3.2",
        "protractor": "~7.0.0",
        "ts-node": "~7.0.0"
    }
}